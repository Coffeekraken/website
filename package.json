{
  "name": "coffeekraken-website",
  "version": "0.0.1",
  "description": "Official coffeekraken website",
  "homepage": "http://coffeekraken.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/Coffeekraken/website"
  },
  "main": "dist/index.js",
  "scripts": {
    "dist": "npm run dist.css && npm run dist.js && npm run dist.img && npm run dist.fonts && npm run dist.html",
    "dist.js": "webpack",
    "dist.css": "node-sass src/sass/style.scss dist/css/style.css  --include-path node_modules --output-style expanded",
    "dist.img": "rm -rf dist/img && node ./.scripts/imagemin.js",
    "dist.html": "cat index.src.html | critical --minify true --inline > index.html",
    "dist.fonts": "rm -rf dist/fonts && cp -R src/fonts dist/fonts",
    "dist.prod": "npm run dist.prod.css && npm run dist.prod.js && npm run dist.img && npm run dist.fonts && npm run dist.html",
    "dist.prod.js": "webpack --config webpack.prod.config.js",
    "dist.prod.css": "node-sass src/sass/style.scss dist/css/style.css  --include-path node_modules --output-style compressed",
    "dist.watch": "concurrently --kill-others \"npm run dist.watch.css\" \"npm run dist.watch.js\" \"npm run dist.watch.img\" \"npm run dist.watch.fonts\" \"npm run dist.watch.html\"",
    "dist.watch.js": "nodemon -w src -e js -x \"npm run dist.js\"",
    "dist.watch.css": "nodemon -w src -e scss -x \"npm run dist.css\"",
    "dist.watch.img": "chokidar 'src/img/**/*' -c \"npm run dist.img\"",
    "dist.watch.fonts": "chokidar 'src/fonts/**/*' -c \"npm run dist.fonts\"",
    "dist.watch.html": "chokidar index.src.html -c \"npm run dist.html\"",
    "carpenter": "coffeekraken-carpenter",
    "start": "concurrently --kill-others \"npm run dist.watch\" \"npm run server\"",
    "server": "http-server .",
    "test": "coffeekraken-testing-stack"
  },
  "keywords": [
    "coffee",
    "kraken",
    "coffeekraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "license": "MIT",
  "dependencies": {
    "coffeekraken-gridle": "git+ssh://git@github.com/Coffeekraken/gridle.git#release/3.0.0",
    "coffeekraken-s-custom-scrollbar-component": "git+ssh://git@github.com/Coffeekraken/s-custom-scrollbar-component.git#release/0.0.1",
    "coffeekraken-s-datepicker-component": "git+ssh://git@github.com/Coffeekraken/s-datepicker-component.git#release/0.0.1",
    "coffeekraken-s-equalize-component": "git+ssh://git@github.com/Coffeekraken/s-equalize-component.git#release/0.0.1",
    "coffeekraken-s-google-map-component": "git+ssh://git@github.com/Coffeekraken/s-google-map-component.git#release/0.0.1",
    "coffeekraken-s-google-map-marker-component": "git+ssh://git@github.com/Coffeekraken/s-google-map-marker-component.git#release/0.0.1",
    "coffeekraken-s-i-typed-component": "git+ssh://git@github.com/Coffeekraken/s-i-typed-component.git#release/0.0.1",
    "coffeekraken-s-ripple-component": "git+ssh://git@github.com/Coffeekraken/s-ripple-component.git#release/0.0.1",
    "coffeekraken-s-share-component": "git+ssh://git@github.com/Coffeekraken/s-share-component.git#release/0.0.1",
    "coffeekraken-sugar": "git+ssh://git@github.com/Coffeekraken/sugar.git#release/0.0.1",
    "github-api": "^3.0.0",
    "marked": "^0.3.6",
    "vue": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "coffeekraken-testing-stack": "git+ssh://git@github.com/Coffeekraken/testing-stack.git#release/0.0.1",
    "concurrently": "^3.1.0",
    "critical": "^0.8.4",
    "http-server": "^0.9.0",
    "imagemin": "^5.2.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.0",
    "node-sass": "4.3.0",
    "nodemon": "^1.11.0",
    "open": "0.0.5",
    "webcomponents.js": "^0.7.23",
    "webpack": "^1.14.0"
  }
}
